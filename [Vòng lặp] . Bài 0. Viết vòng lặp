#include<bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin>>n;
    if(n>=5 && n<=26){
    for( int i=1; i<=n;i++)
    { cout<<i<<" ";}
    cout<<'\n';
    for( int i=n; i>=0;i--)
    { cout<<i<<" ";}
    cout<<'\n';
    for(int i=0;i<=n;i+=2){
        cout<<i<<" ";
    }cout<<'\n';
    for(int i=1;i<=n;i+=2){
        cout<<i<<" ";
    }cout<<'\n';
    for(int i=0;i<=n;i++){
        if(i%4==0&&i<n)cout<<i<<" ";
    }cout<<'\n';
    for(int i=97;i<=n+96;i++){
        cout<<(char)i<<" ";
    }cout<<'\n';
    for(int i=(123-n);i<=122;i++){
        cout<<(char)i<<" ";
    }cout<<'\n';
    
    }
    
    return 0;
}
